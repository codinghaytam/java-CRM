<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.crm.controllers.ShowAgentsController">
    <TableView fx:id="agentTable" layoutY="8.0" prefHeight="412.0" prefWidth="580.0">
        <columns>
            <TableColumn fx:id="cneColumn" prefWidth="118.0" text="CNE" />
            <TableColumn fx:id="nameColumn" prefWidth="71.0" text="Name" />
            <TableColumn fx:id="surnameColumn" prefWidth="114.0" text="Surname" />
            <TableColumn fx:id="passwordColumn" prefWidth="140.0" text="Password" />
        </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
    </TableView>
</AnchorPane>
